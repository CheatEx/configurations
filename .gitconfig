[user]
    name = Alexey Zlobin
    email = alexey.zlobin@gmail.com
[color]
    diff=auto
    interactive=auto
    branch=auto
    ui=true
[color "status"]
    added=green
    changed=magenta
    untracked=yellow
[core]
    autocrlf=false
[alias]
    st=status
    co=checkout
    ci=commit
    nb=checkout -b
    amend=commit --amend
    iadd=add -i
    br=branch
    pretty-log=log --oneline --decorate --graph
    ff=merge --ff-only
[github]
    user = CheatEx

